%h5= "Notes for #{@survey.name}"

#notes-container.admin
  - @notes.each do |note|
    = cell(Note::Cell::Admin, {note: note, patch_url: @patch_url, new_url: @new_url, delete_url: @delete_url, token: @token})

.links
  = link_to "Add Note", @new_url, class: ("add-note btn btn-primary")
  = link_to "New Group", new_admin_survey_survey_group_path(@survey, return_to_notes: true), class: "new-group btn btn-primary"
  = link_to "Return", admin_survey_path(@survey), class: "return btn btn-primary"

- if @template
  #note-template.hidden
    = cell(Note::Cell::Admin, {note: @template, patch_url: @patch_url, new_url: @new_url, delete_url: @delete_url, token: @token})